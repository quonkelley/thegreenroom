{
  "name": "thegreenroom-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:openai": "ts-node test-openai.ts",
    "css:watch": "tailwindcss -i ./styles/globals.css -o ./public/styles/output.css --watch",
    "css:build": "tailwindcss -i ./styles/globals.css -o ./public/styles/output.css",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "quality": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "quality:fix": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "autoprefixer": "^10.4.14",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.0",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.0.2",
    "resend": "^4.6.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/next": "^2.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^2.0.0",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.10.2",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
